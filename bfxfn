	<script>
		const name = document.querySelector(".name");
		var show = true;
		function changeColor() {
			
			if (show) {
				name.style.overflow = "hidden";
				show = false;
			} else {
				name.style.overflow = "auto";
				show = true;
			}
		}
	  </script>
	<script>
        $(document).ready(function(){
        $('.mymultiplediv').mouseover(function() {
        myvar = this.id;
        $("div.mydiv").hide();
        $('#div'+myvar).show();
        });
        });
        </script>
